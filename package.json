{
	"name": "boilerplated",
	"version": "1.0.0",
	"description": "",
	"author": "Stephen Lord",
	"license": "ISC",
	"keywords": [],
	"main": "server/server.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/sglord/boilerplate.git"
	},
	"scripts": {
		"start": "node --experimental-modules --es-module-specifier-resolution=node --experimental-json-modules src/server/server.js",
		"start:dev": "nodemon --experimental-modules --es-module-specifier-resolution=node --experimental-json-modules src/server/server.js",
		"start:dev:concurrent": "concurrently \"nodemon --experimental-modules --es-module-specifier-resolution=node --experimental-json-modules src/server/server.js\" \"webpack --watch --progress\"",
		"build": "webpack",
		"build:watch": "webpack --watch --progress",
		"docker:build": "docker build -t boilerplated .",
		"docker:run": "docker run -p 3000:3000 --name boilerplated boilerplated",
		"lint": "eslint src/**/* --ext .js,jsx",
		"lint:fix": "eslint --fix src/**/* --ext .js,jsx ",
		"prettier": "prettier --check -- src/*/**.js",
		"prettier:write": "prettier --write -- src/*/**.js",
		"test": "jest && npm run lint",
		"test:unit": "jest --debug --collect-coverage",
		"test:unit:watch": "npm run test:unit -- --watch"
	},
	"dependencies": {
		"@babel/polyfill": ">=7.12.1",
		"@babel/runtime": ">=7.12.5",
		"argon2": ">=0.27.0",
		"axios": ">=0.21.0",
		"concurrently": "^5.3.0",
		"cors": ">=2.8.5",
		"express": ">=5.0.0-alpha.8",
		"formik": ">=2.2.5",
		"history": "^5.0.0",
		"jsonwebtoken": ">=8.5.1",
		"mongodb": ">=3.5.7",
		"mongoose": ">=5.9.11",
		"passport": ">=0.4.1",
		"passport-jwt": ">=4.0.0",
		"path": ">=0.12.7",
		"react": ">=17.0.1",
		"react-dom": ">=17.0.1",
		"react-redux": ">=7.2.0",
		"react-router": ">=6.0.0-beta.0",
		"react-router-dom": ">=6.0.0-beta.0",
		"redux": ">=4.0.5",
		"redux-thunk": ">=2.3.0",
		"styled-components": ">=5.1.0"
	},
	"devDependencies": {
		"@babel/core": ">=7.9.6",
		"@babel/plugin-proposal-object-rest-spread": ">=7.9.6",
		"@babel/plugin-proposal-optional-chaining": ">=7.9.0",
		"@babel/plugin-transform-arrow-functions": ">=7.8.3",
		"@babel/plugin-transform-runtime": ">=7.12.1",
		"@babel/preset-env": ">=7.9.6",
		"@babel/preset-react": ">=7.9.4",
		"@babel/register": ">=7.9.0",
		"@commitlint/cli": ">=11.0.0",
		"@commitlint/config-conventional": ">=11.0.0",
		"babel-eslint": ">=10.1.0",
		"babel-jest": ">=26.6.3",
		"babel-loader": ">=8.1.0",
		"babel-plugin-styled-components": ">=1.10.7",
		"babel-plugin-transform-react-remove-prop-types": ">=0.4.24",
		"eslint": ">=7.14.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": ">=6.11.0",
		"eslint-loader": ">=4.0.2",
		"eslint-plugin-babel": ">=5.3.0",
		"eslint-plugin-import": ">=2.20.2",
		"eslint-plugin-jest": ">=24.1.3",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": ">=3.1.3",
		"eslint-plugin-react": ">=7.19.0",
		"eslint-plugin-react-hooks": ">=4.2.0",
		"esm": ">=3.2.25",
		"husky": ">=4.2.5",
		"jest": ">=26.6.3",
		"nodemon": ">=2.0.3",
		"npm-run-all": ">=4.1.3",
		"prettier": ">=2.0.5",
		"redux-devtools-extension": "^2.13.8",
		"webpack": ">=5.9.0",
		"webpack-cli": "^4.2.0",
		"webpack-notifier": ">=1.8.0"
	}
}
